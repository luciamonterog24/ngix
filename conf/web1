server {
    listen 80;
    server_name www.web1.org;
    root /var/www/web1;
    index index.html;
    
    location /privado {
    satisfy any;
    allow 192.168.3.0/24;
    deny all;

    auth_basic "Zona privada";
    auth_basic_user_file /etc/nginx/.htpasswd;
    }

}

server {
    listen 443 ssl;
    server_name www.web1.org;

    ssl_certificate /etc/ssl/certs/web1.crt;
    ssl_certificate_key /etc/ssl/private/web1.key;

    root /var/www/web1;
    index index.html;
}
